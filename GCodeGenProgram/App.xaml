<Application x:Class="GCodeGenProgram.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:GCodeGenProgram"
             xmlns:views="clr-namespace:GCodeGenProgram.Views"
             xmlns:viewmodels="clr-namespace:GCodeGenProgram.ViewModels">
    <Application.Resources>
        <DataTemplate DataType="{x:Type viewmodels:AddNewDrillViewModel}">
            <views:AddNewDrill></views:AddNewDrill>
        </DataTemplate>
        <DataTemplate DataType="{x:Type viewmodels:ChangeSizesViewModel}">
            <views:ChangeSizesView></views:ChangeSizesView>
        </DataTemplate>
        <DataTemplate DataType="{x:Type viewmodels:DrillListViewModel}">
            <views:DrillList></views:DrillList>
        </DataTemplate>
        <DataTemplate DataType="{x:Type viewmodels:MainWindowViewModel}">
            <views:MainWindow></views:MainWindow>
        </DataTemplate>
        <DataTemplate DataType="{x:Type viewmodels:CanvasViewModel}">
            <views:Canvas></views:Canvas>
        </DataTemplate>
    </Application.Resources>
</Application>
